ed_loop :: Rd a $->$ $\{ (\mathsf{a} -> \mathsf{b}) \mid \lozenge \mathsf{W@a2r}\}$ $->$ b $|>$ R

${-}{-}\ \langle \mu (\mathsf{R@z2a; W@a2z \oplus W@a2r}) \rangle$
let A =
  let f v = match v with
          | ('Tick, 0) $=>$ wr () $->$ a2z
          | ('Tick, 1) $=>$ wr ($\bullet$ :: string$)->$ a2r in
in ed_loop z2a f

