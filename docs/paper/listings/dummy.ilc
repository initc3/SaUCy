dummy :: $\forall$ a ... . $\tyNat -> [\tyBit] -> \keyword{Crupt} -> \cdots$ a
let dummyA k bits crupt toZ toF toP toQ toQasP frZ frF frP frQ toPasQ=
  let fwd2Z $\eUnit$ c = loop ($\lambda$ m . wr (X2Z m) $->$ toZ) c in
       loop ($\lambda$ x . match x with
		 | A2F m $=>$ wr m $->$ toF
		 | A2P m $=>$ if crupt == CruptP
			   $\,$then wr m $->$ toQasP
			   $\,$else wr m $->$ toP) frZ
    $\xFork$ fwd2Z $\eUnit$ frF
    $\xFork$ fwd2Z $\eUnit$ frP
    $\xFork$ fwd2Z $\eUnit$ frQ
