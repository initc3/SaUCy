execUC :: $\forall$ ... . $\tyNat -> [\tyBit] -> \keyword{Crupt} -> X_{\mathsf{z}} -> A_{\mathsf{p}} \times A_{\mathsf{q}}$
              $-> A_{\mathsf{f}} -> A_{\mathsf{a}} {->}_{m} \tyBit $
let execUC k r crupt z (p,q) f a =
  $\nu$ ... . let ... in
       let (rf,ra,rp,rq,rz) = splitBits5 r in
	 $\ \ \ \,$ f k rf crupt ...
	 $\xFork$ a k ra crupt ...
	 $\xFork$ wrapperP p k rp (crupt == CruptP) ...
	 $\xFork$ wrapperQ q k rq (crupt == CruptQ) ...
	 $\xFork$ z k rz ...