execUC :: $\forall$ ... . $X_{\mathsf{z}} \multimap X_{\mathsf{p}} \otimes X_{\mathsf{q}} \multimap X_{\mathsf{f}} \multimap X_{\mathsf{a}}$
                 $\multimap \tyBang{\tyNat} \multimap \tyBang{[\tyBit]} \multimap \tyBang{\keyword{Crupt}} \multimap_{\mathsf{m}_{\mathsf{e}}} \mathsf{a} $ 
let execUC z (p,q) f a k !r !crupt =
  $\nu$ ... . let (rf,ra,rp,rq,rz) = splitBits5 r in
	 $\ \ \ \,$ f !crupt ... k !rf
	 $\xFork$ a !crupt ... k !ra
	 $\xFork$ wrapper p !(crupt == CruptP) ... k !rp
	 $\xFork$ wrapper q !(crupt == CruptQ) ... k !rq
	 $\xFork$ z ... k !rz