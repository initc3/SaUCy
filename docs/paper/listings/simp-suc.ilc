execUC :: $\forall$ ... . $\tyNat\ {->}_{\wm}\ [\tyBit] -> \keyword{Crupt} -> A_{\mathsf{z}} -> A_{\mathsf{p}} \times A_{\mathsf{q}}$
              $-> A_{\mathsf{f}} -> A_{\mathsf{a}} -> \tyBit $
let execUC k r crupt z (p,q) f a =
  $\nu$ ... . let ... in
       let (rf,ra,rp,rq,rz) = splitBits r in
	 $\ \ \ \,$ f k rf crupt ...
	 $\xFork$ a k ra crupt ...
	 $\xFork$ corruptOrNot p k rp (crupt == CruptP) ...
	 $\xFork$ corruptOrNot q k rq (crupt == CruptQ) ...
	 $\xFork$ z k rz ...
