execUC :: $\forall$ ... . $A_{\mathsf{z}} {->}_{\wm}\ A_{\mathsf{p}} \times A_{\mathsf{q}}-> A_{\mathsf{f}} -> A_{\mathsf{a}} ->$
              $\mathsf{Crupt} -> \mathsf{Nat} -> [\tyBit] -> \tyBit $
let execUC z (p,q) f a crupt k r =
  $\nu$ ... . let (rf,ra,rp,rq,rz) = splitBits r in
	 $\ \ \ \,$ f k rf crupt ...
	 $\xFork$ a k ra crupt ...
	 $\xFork$ corruptOrNot p k rp (crupt == CruptP) ...
	 $\xFork$ corruptOrNot q k rq (crupt == CruptQ) ...
	 $\xFork$ z k rz ...
