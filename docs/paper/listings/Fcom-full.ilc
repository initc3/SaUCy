fCom :: $\tyNat -> [\tyBit] -> \mathsf{Crupt} -> \cdots \multimap_{\textsf{\tiny R}} \mathbbm{1}$
let fCom k bits crupt toP toQ toA frP frQ frA =
  let $\langle$!(Commit b), frP$\rangle$ = rd frP in
    wr Receipt $->$ toQ ;
    let $\langle$!Open, frP$\rangle$ = rd frP in
      wr (Opened b) $->$ toQ